
<style lang="scss">
    .payment_wrap{
        border-top:2rpx solid #e0e0e0;
    }
    .payment{
        &_wrap{
            padding: 0 30rpx;
            padding-bottom: 105rpx;
            box-sizing: border-box;
        }
        &_top{
            height: 100%;
        }
        &_bar{
            display: flex;
            width: 100%;
            height: 100rpx;
            line-height: 100rpx;
            border-top: 1px solid #ccc;
            background-color: #fff;
            position: fixed;
            left: 0;bottom: 0;
            &_price{
                flex-grow: 1;
                text-align: right;
                padding-right: 36rpx;
                color: #fc2626;
                display: flex;
                font-size: 32rpx;
                view{
                    flex: 1;
                    text{
                        font-size: 24rpx;
                    }
                }
            }
            &_btn{
                width: 160rpx;
                flex-shrink: 0;
                text-align: center;
                font-size: 36rpx;
                color: #fff;
                background-color: #d64723;
            }
        }
        &_msg{
            .address_btn{
                display: block;
                width: 100%;
                font-size: 24rpx;
                color: #999999;
                height: 100rpx;
                line-height: 100rpx;
                border-bottom: 1px solid #e0e0e0;
                .xnrDizhi{
                    float: left;
                    margin-right: 17rpx;
                }
            }
            &_address{
                display: flex;
                border-bottom: 1px solid #e0e0e0;
                padding-bottom:20rpx;
                &_msg{
                    flex-grow: 1;
                }
                &_name{
                    display: flex;
                    view{
                        font-size: 28rpx;
                        display: inline-block;
                        vertical-align: middle;
                        margin-left: 14rpx;
                        padding-top: 16rpx;
                    }
                }
                &_info{
                    font-size: 26rpx;
                    padding: 19rpx 16rpx 12rpx 16rpx;
                }
                &_left_icon,
                &_right_icon{
                    width: 60rpx;
                    text-align: center;
                    font-size: 36rpx;
                    color: #999;
                    flex-shrink: 0;
                    &:after{
                        content: '';
                        display: inline-block;
                        height: 100%;
                        vertical-align: middle;
                    }
                    view{
                        display: inline-block;
                        vertical-align: middle;
                    }
                }
                &_right_icon{
                    width: 40rpx;
                    font-size: 16rpx;
                    margin-top:30rpx;
                }
            }
        }
        &_ticket{
            padding: 14rpx;
            .coupon_tit{
                display: block;
                line-height: 36rpx;
                font-size: 24rpx;
                border-left: 8rpx solid #ee3b3b;
                text-indent: .5em;
            }
            .coupon_scroll {
                padding-left: 0;
                padding-right: 0;
            }
            &_not{
                font-size: 32rpx;
                text-align: center;
            }
        }
        &_coupon_btn{
            line-height: 50rpx;
            padding: 15rpx 0;
            display: flex;
            justify-content: space-between;
            font-size: 32rpx;
            text:first-child{
                border-left: 10rpx solid #ee3b3b;
                padding-left: 14rpx;
            }
        }
        @at-root{
            .coupon_list{
                &_wrap{
                    width: 100%;
                    height: 100%;
                    position: fixed;
                    left: 0;top: 0;
                    z-index: 9;
                    .mask{
                        width: 100%;
                        height: 100%;
                        position: absolute;
                        left: 0;top: 0;
                        background: rgba(0, 0, 0, .5);
                    }
                }
                &_title{
                    height: 80rpx;
                    line-height: 80rpx;
                    color: #ee3b3b;
                    text-align: center;
                }
                &_cont{
                    height: 564rpx;
                    &_wrap{
                        width: 100%;
                        position: absolute;
                        left: 0; bottom: 0;
                        background: #fff;
                        padding-bottom: 12rpx;
                    }

                }
                &_items{
                    margin: 0 30rpx;
                    padding: 24rpx 0 10rpx;
                    border: 1px solid #f05466;
                    border-radius: 14rpx;
                    margin-bottom: 20rpx;
                    color: #f05466;
                    background: {
                        repeat: repeat-x;
                        image: url(data:image/png;base64,);
                    }
                    position: relative;
                    .selected{
                        font-size: 50rpx;
                        font-weight: bolder;
                        position: absolute;
                        top: 30rpx; right: 20rpx;
                    }
                    .coupon_list_items_msg_time{
                        color: #333;
                    }
                    &_not{
                        color: #ccc;
                        border: 1px solid #ccc;
                        .coupon_list_items_msg_time{
                            color: #ccc;
                        }
                        background-image: url(data:image/png;base64,);
                    }

                    &_name{
                        padding-left: 30rpx;
                    }
                    &_msg{
                        display: flex;
                        &_price{
                            width: 200rpx;
                            height: 160rpx;
                            line-height: 160rpx;
                            padding-right: 40rpx;
                            text-align: right;
                            font-size: 80rpx;
                            flex-shrink: 0;
                        }
                        &_time{
                            flex-grow: 1;
                            display: flex;
                            align-items: center;
                            flex-wrap: wrap;
                            view{
                                width: 100%;
                            }
                        }
                    }
                }
            }
        }
    }
    .icon_size{
        font-size: 156rpx;
    }
</style>

<template>

    <view class="payment_wrap">

        <view class="payment_msg">

            <view @tap="openAddress" class="address_btn" wx:if="{{!address.userName}}">
                <view class="xnrDizhi">
                    <xnricon type="dizhi" color="#999999" fontSize="50"></xnricon>
                </view>
                请选择收货地址 >
            </view>
            <view @tap="openAddress" class="payment_msg_address" wx:if="{{address.userName}}">
                <view class="payment_msg_address_left_icon">
                    <xnricon3 type="dizhi" color="#999999" fontSize="50"></xnricon3>
                </view>
                <view class="payment_msg_address_msg" >

                    <view class="payment_msg_address_info">
                        {{address.provinceName}}{{address.cityName}}{{address.countyName}}{{address.detailInfo}}
                    </view>

                    <view class="payment_msg_address_name">
                        <view>{{address.userName}}</view>
                        <view>{{address.telNumber}}</view>
                    </view>

                </view>
                <view class="payment_msg_address_right_icon">
                    <xnricon1 type="jiantouyou" color="#999999" fontSize="50"></xnricon1>
                </view>
            </view>
        </view>

        <view class="payment_top">
            <repeat for="{{payDataMsg}}">
                <pay-items :payItemsConfig="item" :index.sync="index" @changeNum.user="changeNum"></pay-items>
            </repeat>

            <view class="payment_coupon_btn" wx:if="{{couponBtnOpen}}" @tap="openCoupon">
                <text>优惠券</text>
                <text>{{couponPrice}}</text>
            </view>

        </view>
        <!-- 优惠券列表 -->
        <view class="coupon_list_wrap" wx:if="{{couponNot}}">
            <view class="mask" @tap="closeCoupon"></view>

            <view class="coupon_list_cont_wrap" catchtap="catch">
                <view class="coupon_list_title">
                    优惠券
                </view>
                <scroll-view class="coupon_list_cont" scroll-y>

                    <repeat for="{{couponList}}">
                        <view class="coupon_list_items {{!item.status ? 'coupon_list_items_not' : ''}}" id="{{index}}" data-status="{{item.status}}" @tap="selected">
                            <view class="coupon_list_items_name">{{item.name}}</view>
                            <view class="coupon_list_items_msg">
                                <view class="coupon_list_items_msg_price">
                                    {{item.money}}
                                </view>
                                <view class="coupon_list_items_msg_time">
                                    <view>满{{item.terms}}元可用</view>
                                    <view>(即日 - {{item.cancelTime}})</view>
                                </view>
                            </view>

                            <view wx:if="{{item.selected}}" class="selected iconfont icon-chenggong"></view>

                        </view>
                    </repeat>
                </scroll-view>
            </view>
        </view>

        <view class="payment_bar">
            <view class="payment_bar_price">

                <view>
                    <text>已优惠: </text>{{discount}}
                </view>
                <view>
                    <text>应付: </text>{{realPrice}}
                </view>

            </view>
            <view class="payment_bar_btn" @tap="payActive">
                结算
            </view>
        </view>
        <toast />
        <modal />
    </view>

</template>

<script>

    import wepy from 'wepy'
    import PayItems from '../components/PayItems'
    import retainedDecimalMoney from '../assets/retainedDecimalMoney'
    import getExtJson from '../assets/getExtJson'
    import Toast from 'wepy-com-toast'
    import Modal from '../components/Modal'
    import inspectUserLogin from '../assets/inspectUserLogin'
    import Coupon from '../components/Coupon'
    import payMethod from '../assets/pay' //付款方法
    import xnricon from 'xnr-icon'

    export default class Payment extends wepy.page{
        config = {
            navigationBarTitleText: '购买'
        }
        components = {
            'pay-items': PayItems,
            toast: Toast,
            modal: Modal,
            coupon: Coupon,
            xnricon3: xnricon,
            xnricon1: xnricon

        }
        data = {
            payDataMsg: [],
            allPrice: '￥0',
            discount: '￥0',
            realPrice: '￥0',
            couponPrice: '',
            couponList: [],
            address: {},
            couponConfig: [],
            selectedCoupon: null,
            couponPrice: '暂无优惠券可用',
            couponNot: false,
            couponBtnOpen: false,
            ping: false
        }
        methods = {
            catch(){},
            changeNum(e){
                console.log(e)
                let payGoods = wepy.getStorageSync('payGoods')
                if(e.plus){
                    this.payDataMsg[e.id].quantity = payGoods[e.id].quantity++

                    this.payDataMsg[e.id].quantity = payGoods[e.id].quantity = payGoods[e.id].quantity >= payGoods[e.id].stock ? payGoods[e.id].stock : payGoods[e.id].quantity

                    this.realPrice = this.allPrice = this.payDataMsg[e.id].allPrice = payGoods[e.id].allPrice = retainedDecimalMoney(payGoods[e.id].quantity * payGoods[e.id].price)

                }else if(e.reduce){
                    this.payDataMsg[e.id].quantity = payGoods[e.id].quantity--

                    this.payDataMsg[e.id].quantity = payGoods[e.id].quantity = payGoods[e.id].quantity <= 1 ? 1 : payGoods[e.id].quantity

                    this.realPrice = this.allPrice = this.payDataMsg[e.id].allPrice = payGoods[e.id].allPrice = retainedDecimalMoney(payGoods[e.id].quantity * payGoods[e.id].price)

                }
                wepy.setStorageSync('payGoods', payGoods)
                this.available()
                this.$apply()
            },
            openAddress(){
                wepy.chooseAddress({
                    success: (res) => {
                        this.address = res
                        wepy.setStorageSync('normalAddress', res)
                        this.$apply()
                    }
                })
            },
            openCoupon(){
                if(this.couponList.length > 0) this.couponNot = true
            },
            closeCoupon(){
                this.couponNot = false
            },
            payActive(){
                let payGoods = wepy.getStorageSync('payGoods').map( item => {
                    return {
                        goods_id: item.goods_id,
                        goods_name: item.goods_name,
                        skucode: item.skucode,
                        props: item.props,
                        price: item.price,
                        quantity: item.quantity,
                        thumb: item.thumb
                    }
                })

                // 检测用户是否登录
                if(!this.inspectUserLogin){
                    this.$invoke('modal', 'show', {
                        content: '您还没有授权登录',
                        specialType: 'getUserInfo',
                        specialText: '登录',
                        showCancel: false,
                        success: (res) => {
                            if(res.data.userInfo){
                                this.inspectUserLogin = true
                                this.$parent.getUserInfo( res => {
                                    console.log(res)
                                })
                            }
                        }
                    })
                    return
                }


                // 付款传的参数
                const submitData = {
                    items: payGoods,
                    config_id: this.config_id,
                    real_name: this.address.userName, //自己的姓名
                    mobile: this.address.telNumber,
                    province: this.address.provinceName,
                    city: this.address.cityName,
                    county: this.address.countyName,
                    address: this.address.detailInfo,
                    zip: this.address.postalCode,
                    goods_price: +this.allPrice.substr(1),
                    openid: this.openid,
                    source: '小程序'
                }

                // 检测是否使用优惠券
                if(this.selectedCoupon){
                    submitData.coupon_id = this.selectedCoupon.id
                    submitData.coupon_price = this.selectedCoupon.money
                }

                // 检测是否选择发货地址
                if(!submitData.address) {
                    this.$invoke('toast', 'show', {
                        title: '地址未选择',
                        imgClassName: 'iconfont icon-shibaibiaoqing icon_size'
                    })
                    return
                }
                //  /restapi/wechat-pinkshop-order/create
                // 判断是否拼团
                if(this.pin){
                    console.log('pin')

                    submitData.groups = [
                        {
                            nickname: this.$parent.globalData.userInfo.nickName,
                            touxiang: this.$parent.globalData.userInfo.avatarUrl,
                            parent: this.parent
                        }
                    ]
                    submitData.active_id = this.activeId
                    submitData.is_cluster = this.cluster
                    submitData.config_id = this.config_id
                    wepy.request({
                        url: `${this.api_host}/restapi/wechat-pinkshop-order/create`,
                        method: 'POST',
                        data: submitData,
                        success: res => {
                            if (!res.data.success) {
                                this.$invoke("toast", "show", {
                                    title: res.data.data.message,
                                    imgClassName: "iconfont icon-shibaibiaoqing icon_size"
                                });
                                return;
                            }
                            wepy.requestPayment({
                                timeStamp: res.data.data.timeStamp,
                                nonceStr: res.data.data.nonceStr,
                                package: res.data.data.package,
                                signType: res.data.data.signType,
                                paySign: res.data.data.paySign,
                                success: res => {
                                    wepy.showToast({
                                        title: "支付成功",
                                        icon: "success",
                                        duration: 1000
                                    })
                                    // paySuccess;
                                    wepy.navigateTo({
                                        url: "paySuccess?text=1",

                                    })
                                },
                                fail: res => {
                                    // err_desc
                                    this.$invoke("toast", "show", {
                                        title: "支付失败",
                                        imgClassName: "iconfont icon-shibaibiaoqing",
                                        fontSize: 188
                                    })
                                }
                            });
                        }
                    })

                }else if(this.seckill){
                    console.log('秒杀')
                    // /restapi/wechat-seckill-order/create
                    submitData.seckill_id = this.activeId
                    wepy.request({
                        url: `${this.api_host}/restapi/wechat-seckill-order/create`,
                        method: 'POST',
                        data: submitData,
                        success: res => {
                            if (!res.data.success) {
                                this.$invoke("toast", "show", {
                                    title: res.data.data.message,
                                    imgClassName: "iconfont icon-shibaibiaoqing icon_size"
                                });
                                return;
                            }
                            wepy.requestPayment({
                                timeStamp: res.data.data.timeStamp,
                                nonceStr: res.data.data.nonceStr,
                                package: res.data.data.package,
                                signType: res.data.data.signType,
                                paySign: res.data.data.paySign,
                                success: res => {
                                    wepy.showToast({
                                        title: "支付成功",
                                        icon: "success",
                                        duration: 1000
                                    })
                                    // paySuccess;
                                    wepy.navigateTo({
                                        url: "paySuccess?text=1",

                                    })
                                },
                                fail: res => {
                                    // err_desc
                                    this.$invoke("toast", "show", {
                                        title: "支付失败",
                                        imgClassName: "iconfont icon-shibaibiaoqing",
                                        fontSize: 188
                                    })
                                }
                            });
                        }
                    })
                }else {
                    console.log('ok')
                    payMethod.call(this, {
                        data: submitData,
                        complete: () => {
                            const payGoods = wepy.getStorageSync('payGoods')
                            const allGoods = wepy.getStorageSync('selectedGoods')
                            payGoods.forEach( pay => {
                                allGoods.items.forEach( (all, index) => {
                                    if(pay.goods_id == all.goods_id){
                                        allGoods.items.splice(index, 1)
                                    }
                                });
                            });
                            wepy.setStorageSync('selectedGoods', allGoods)
                        }
                    })
                }


            },
            selected(e){
                const id = e.currentTarget.id
                const selectedCoupon = this.couponList[id]
                if(!e.currentTarget.dataset.status) return
                this.selectedCoupon = {
                    money: selectedCoupon.money,
                    id: selectedCoupon.id
                }
                this.discount = retainedDecimalMoney(selectedCoupon.money)
                let money = selectedCoupon.money.split('.')
                this.couponPrice = +money[1] > 0 ? `${selectedCoupon.money}元` : `${money[0]}元`

                this.couponList.forEach( (item, index) => {
                    if(id == index){
                        this.couponList[index].selected = true
                    }else {
                        this.couponList[index].selected = false
                    }
                });
                let newAllPrice = parseFloat(this.allPrice.substr(1)) - parseFloat(this.discount.substr(1))
                this.realPrice = retainedDecimalMoney(newAllPrice)
                this.couponNot = false
                this.$apply()
            }
        }
        getCoupon(){
            wepy.request({
                url: `${this.api_host}/restapi/wechat-my-coupons`,
                data: {
                    config_id: this.config_id,
                    apps_openid: this.openid,
                    status:1,
                    expand: 'coupon'
                },
                success: (res) => {
                    this.couponBtnOpen = res.data.data.length > 0 ? true : false
                    this.couponConfig.length = 0
                    res.data.data.forEach( items => {
                        let item = items.coupon
                        let money = item.face_value.split('.')
                        console.log()
                        this.couponConfig.push({
                            id: items.id,
                            name: item.coupon_name,
                            terms: item.order_odds,
                            money: +money[1] > 0 ? money.join('.') : money[0],
                            cancelTime: item.expire_time.split(' ')[0]
                        })
                    })
                    this.available()
                }
            })
        }
        available(){
            let cruuentPrice = parseFloat(this.allPrice.substr(1))
            this.couponConfig.forEach( item => {
                if(parseFloat(item.terms) <= cruuentPrice) {
                    this.couponPrice = '有可用'
                    item.status = true
                }
                this.couponList.push(item)
            });
            console.log(this.couponList)
            this.$apply()
        }
        onShow(){
            // 获取登录状态
            inspectUserLogin.call(this)
        }
        onLoad(options){
            console.log(options)
            this.pin = options.pin == 'true' ? true : false
            this.seckill = options.seckill == 'true' ? true : false
            this.activeId = options.id
            this.parent = options.parent
            this.cluster = options.cluster

            this.payDataMsg = wepy.getStorageSync('payGoods')
            this.realPrice = this.allPrice = retainedDecimalMoney(this.payDataMsg.reduce( (normal, item) => {
                return (+item.price * item.quantity) + normal
            },0))
            const normalAddress = wepy.getStorageSync('normalAddress')
            this.address = normalAddress ? normalAddress : {}
            getExtJson.call(this)

            if(!this.pin && typeof this.pin !== 'undefined' || !this.seckill && typeof this.seckill !== 'undefined'){ // 不是拼团时获取优惠券
                console.log(!this.seckill && typeof this.seckill !== 'undefined')
                this.getCoupon()
            }

            this.$apply()
        }
    }


</script>


